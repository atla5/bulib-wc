{
  "name": "bulib-webcomponents",
  "version": "0.9.0",
  "description": "collection of web components and helpers used at Boston University Libraries",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "@open-wc/building-webpack": "^0.1.9",
    "@open-wc/polyfills-loader": "^0.2.2",
    "http-server": "^0.11.1",
    "owc-dev-server": "^0.1.6",
    "polymer-cli": "^1.9.6",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.2.0",
    "webpack-cli": "^3.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bulib/bulib-wc.git"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "lit-html",
    "polymer",
    "code4lib"
  ],
  "author": "@bulib",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bulib/bulib-wc/issues"
  },
  "homepage": "https://github.com/bulib/bulib-wc#readme",
  "scripts": {
    "build": "webpack index.js --output ./dist/bundle.js",
    "start": "webpack-dev-server --mode development --open",
    "start:es5": "webpack-dev-server --mode development --es5",
    "start:build": "http-server dist/ -o",
    "build:stats": "webpack --mode production --profile --json > bundle-stats.json"
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
